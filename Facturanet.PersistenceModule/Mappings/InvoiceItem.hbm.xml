<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Facturanet.Core" namespace="Facturanet.Entities">
  <class name="InvoiceItem">
    <id name="Id">
      <generator class="identity"/>
    </id>
    <property name="Quantity" />
    <property name="BasePrice" />
    <many-to-one name="Product" column="IdProduct"/>
    <many-to-one name="Invoice" column="IdInvoice"/>
  </class>
</hibernate-mapping>
